{
  "name": "eslit",
  "version": "4.2.0",
  "description": "ES6 Template Strings + Promises",
  "author": "Jonathan Neal <jonathantneal@hotmail.com> (http://jonathantneal.com)",
  "license": "CC0-1.0",
  "repository": "jonathantneal/eslit",
  "homepage": "https://github.com/jonathantneal/eslit#readme",
  "bugs": "https://github.com/jonathantneal/eslit/issues",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "prototype"
  ],
  "scripts": {
    "clean": "git clean -X -d -f",
    "lint": "npm run lint:ec && npm run lint:js",
    "lint:ec": "echint",
    "lint:js": "eslint *.js lib/*.js --cache --ignore-pattern .gitignore",
    "pre-commit": "echo 'Running pre-commit checks...' && exit 0",
    "prepublish": "npm test",
    "tape": "cd test && npm install && cd .. && node test",
    "test": "npm run lint && npm run tape"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "echint": "^4.0.1",
    "echint-config-dev": "1.0.0",
    "eslint": "^4.5.0",
    "eslint-config-dev": "^2.0.0",
    "pre-commit": "^1.2.2"
  },
  "echint": {
    "extends": "dev"
  },
  "eslintConfig": {
    "extends": "dev"
  },
  "pre-commit": [
    "pre-commit",
    "lint"
  ],
  "keywords": [
    "babel",
    "imports",
    "literals",
    "lits",
    "parses",
    "parsing",
    "resolves",
    "resolving",
    "sugars",
    "tagged",
    "templates",
    "templatings"
  ]
}
